n=int(input("Enter a number")
s=0
while(n!=0):
    r=n%10
    s=s+r*r
    n=n//10
print(s)
if(s==1 or s==7):
    print("happy number")
else:
    print("not a happy number")
num=int(input("Enter a number:"))
a,b=0,1
print("Fibonacci series:",a,b,end=" ")
while True:
    a,b=b,a+b
    if(b>num):
        break
    print(b,end="  ")
    
