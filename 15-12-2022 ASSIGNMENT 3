n=int(input("enter the number of elements="))
l=list(map(int,input("enter list elements=").split()));
k=int(input("Enter a key value"))
choice=0
for i in range(0,n):
    for j in range(i+1,n):
      if(l[i]-l[j]==k):
         choice=1
         break
if(choice==1):
    print("Yes")
else:
    print("No")
